      <!-- -------------------- Start Form  for Admin login---------------- -->
    <div id="auth">
        
<div class="container">
    <div class="row">
        <div class="col-md-5 col-sm-12 mx-auto">
            <div class="card pt-8">
                <div class="card-body">
                    <div class="text-center mb-5">
                        <img src="img/logo.png" height="70px" class='mb-4'>
                        <h3>Sign In Admin </h3>

                        <p>Please sign in to continue to Dashbord Admin</p>
                    </div>
                    <form action="index.html">
                        <div class="form-group position-relative has-icon-left">
                            <label for="username">Email</label>
                            <div class="position-relative" >
                                <input type="email" class="form-control"  required placeholder="Enter Your Email"> 

                                <div class="form-control-icon">
                                    <i data-feather="user"></i>
                                </div>
                            </div>
                        </div>
                        <div class="form-group position-relative has-icon-left">
                            <div class="clearfix-1">
                                <label for="password">Password</label>
                                <input type="password" class="form-control"  required placeholder="Enter Password">


                             
                            </div>
                            <div class="position-relative">
                                <div class="form-control-icon">
                                    <i data-feather="lock"></i>
                                </div>
                            </div>
                        </div>

                     
                          
                        </div>
                        <div class="clearfix">
                            <button type="submit" class="btn btn-primary float-right center">Submit</button>
                        </div>
                    </form>
                   
                  
                </div>
            </div>
        </div>
    </div>
</div>





